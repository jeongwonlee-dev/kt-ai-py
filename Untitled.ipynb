    "def solution(store, customer):\n",
    "    answer = []\n",
    "    \n",
    "    for i in customer:\n",
    "        if i in store:\n",
    "            answer.append('yes')\n",
    "        else:\n",
    "            answer.append('no')\n",
    "    return answer\n",
    "\n",
    "store = [2,3,7,8,9]\n",
    "customer = [7,5,9]\n",
    "solution(store, customer)"
   ]
